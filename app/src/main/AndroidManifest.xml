<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.launchcontrol">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_rocket"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.LaunchControl">
        <activity android:name=".SpaceXActivity" />
        <activity android:name=".TwrActivity" />
        <activity android:name=".DeltaVActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>